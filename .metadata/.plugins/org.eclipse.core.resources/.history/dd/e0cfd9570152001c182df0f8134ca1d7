			juntarCartasSobrantes();&#xD;
			actualizarPuntosJugadores();&#xD;
			if (this.estadoJuego != Estado.FINALIZADO) {&#xD;
				actualizarGanadorRonda();	&#xD;
				notificarEvento(EventosEscoba.RONDAFINALIDAZA);&#xD;
				reparteGeneral();&#xD;
			}&#xD;
			else {&#xD;
