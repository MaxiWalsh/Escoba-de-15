              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mpXfsFH6EeyMo-Y4_C88Kg" name="cambiarAJuegoFinalizado" specification="_mmtNQFH6EeyMo-Y4_C88Kg">
                <language>java</language>
                <body>boolean juegoTerminado = false;&#xD;
		Integer i = 0;&#xD;
		while ((i &lt; jugadores.size()) &amp;&amp; !(juegoTerminado)) {&#xD;
			if ( jugadores.get(i).getPuntos() >= this.puntajeObjetivo) { &#xD;
				juegoTerminado = true;&#xD;
				this.ganador = jugadores.get(i);&#xD;
				this.estadoJuego = Estado.FINALIZADO;&#xD;
				notificarEvento(EventosEscoba.JUEGOFINALIZADO);&#xD;
			}&#xD;
			i++;&#xD;
		}&#xD;
	</body>
              </ownedBehavior>
